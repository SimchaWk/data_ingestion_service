MATCH (a:Attack {id: '7316f9598a774574b84cd699c421ab1d'}), (l:Location {country: 'United States', city: 'New York City'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: '7efc106da75c4f72aa05a29a84052247', date: '1981-09-07T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'UNITED STATES.  A small bomb knocked out the window of a Manhattan bookstore that specializes in books published in the Soviet Union.  There were no injuries.  A militant Jewish group called the Thunder of Zion, protesting the treatment of Soviet Jews, claimed credit for the bombing. The group demanded the release of two dissident Soviet Jews, or else Soviet blood will flow in the streets of New York. The bomb was the eighth anti-Soviet bomb planted in New York that week.'});

