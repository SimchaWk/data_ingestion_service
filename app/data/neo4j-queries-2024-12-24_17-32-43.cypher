CREATE (a:Attack {id: '0ce81d0d14694511ba48a594b63ad6d5', date: '1981-06-01T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'ISRAEL.  A suspicious package found in a Jerusalem cafe turned out to be a bomb.  It exploded, wounding a bomb disposal specialist who was attempting to dismantle it.'});

