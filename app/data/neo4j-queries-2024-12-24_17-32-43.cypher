MATCH (a:Attack {id: '362852b9437f418aa5c10c0abab300f4'}), (l:Location {country: 'Federal Republic Of Germany', city: 'Karlsbad'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: 'b30ebdb8abc94927916cd7eca4758bf7', date: '1982-10-21T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: "GREECE.  An anarchist organization calling itself Autonomy warned that it would place bombs in various points of Athens, in particular those of U.S. interest, such as the American Union.  The organization's leaflets, in which the plot was mentioned, were found behind the statue of Elevtherios Venizelos.  Unidentified people also phoned newspaper offices about the plot, at which time the newspapers informed the police who discovered the bombs only at the American Union.  The motive was apparently revenge because pre-election announcements regarding the abolition of American bases from Greece, Greece's withdrawal from NATO, etc., had not been implemented. The bomb at the American Union was disarmed by experts."});

MATCH (a:Attack {id: 'b30ebdb8abc94927916cd7eca4758bf7'}), (l:Location {country: 'Greece', city: 'Athens'}) CREATE (a)-[:OCCURRED_AT]->(l);

