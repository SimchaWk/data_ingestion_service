CREATE (a:Attack {id: 'b4956065ae7e429da7f6df7d88879e6e', date: '1980-06-01T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: "UNITED KINGDOM.  A bomb exploded in London's West End at the offices of the Kuwait Oil Company.  Damage was reported but no injuries.  The one-pound bomb blew out the front of the building at a time when virtually no one was in the area."});

