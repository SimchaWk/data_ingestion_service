MATCH (a:Attack {id: 'dc8982ab47a64a888b4d21cb78eed751'}), (l:Location {country: 'Federal Republic Of Germany', city: 'Munich'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: '561aaf1dea2446c3b6ec6e058eb8295f', date: '1984-01-27T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'FRANCE.  The Moroccan Consulate in Montpellier was the target of four Molotov cocktails.  The bombs, which were thrown very early in the morning, caused a fire in the building.  The attackers sent a message to the local newspaper protesting the silence of the French press over the alleged bloody repression of the recent riots in Morocco.'});

MATCH (a:Attack {id: '561aaf1dea2446c3b6ec6e058eb8295f'}), (l:Location {country: 'France', city: 'Montpellier'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: 'b5e5d427869c4fb3aa0d6820b267c82d', date: '1984-01-30T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'ISRAEL.  Police found explosives on Temple Mount after what sources described as an attempt by a fanatic Jewish group to blow up the two major Islamic mosques at the site in east Jerusalem.'});

