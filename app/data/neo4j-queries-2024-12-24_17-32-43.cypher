CREATE (a:Attack {id: 'f293e0bd45b943c19cdd5c35471d16a2', date: '1981-11-26T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: "COLOMBIA.  M-19 guerrillas attacked an oil exploration camp near Orito. Nelson Rodriquez Pinillas, a geologist employed by the U.S.-owned Western Petroleum Corporation, was assassinated during this action.  He was accused of being an 'army informer'."});

MATCH (a:Attack {id: 'f293e0bd45b943c19cdd5c35471d16a2'}), (l:Location {country: 'Colombia', city: 'Orito'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: '27ef407cec294d1588e56a6b3b45e302', date: '1981-11-30T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'ANGOLA.  The Union for the Total Independence of Angola (UNITA) bombed a Belgian-owned oil refinery in Luanda as a part of its struggle to destroy the economic base of the current Marxist government which counts on oil production for 80% of its foreign exchange earnings.  The sabotage team managed to destroy one of the three distillation units, an act which is expected to put the refinery out of operation for at least two months.'});

MATCH (a:Attack {id: '27ef407cec294d1588e56a6b3b45e302'}), (l:Location {country: 'Angola', city: 'Luanda'}) CREATE (a)-[:OCCURRED_AT]->(l);

