CREATE (a:Attack {id: 'c2dee74d20274af6928efba2ee1e6fdc', date: '1980-04-08T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'GUATEMALA.  Several unidentified individuals riding in a car attacked the Japanese embassy in Guatemala City.  Several bullets shot from the moving vehicle broke the main window and hit the walls.  There were no injuries.'});

