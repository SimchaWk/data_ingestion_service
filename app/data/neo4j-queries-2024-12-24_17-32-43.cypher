MATCH (a:Attack {id: '93e487f3dcb944558fce11d965cdeac6'}), (l:Location {country: 'German Democratic Republic', city: 'East Berlin'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: '3b7c467d72aa44268668ef86f0e0ff94', date: '1986-05-07T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: "LEBANON.  A retired French auto dealer was kidnapped in Beirut by a group calling itself the Revolutionary Justice Organization, reportedly a cover for Hezbollah.  Camille Sontag was traveling with his wife, Blanche, when they were intercepted by two gunmen in militia uniforms.  One held a gun to Mrs. Sontag's head and the other pulled her husband from the car.  Sontag was released by his captors November 11, 1986."});

