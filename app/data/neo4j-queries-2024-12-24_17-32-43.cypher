CREATE (a:Attack {id: 'f8af03acb8704fefa4f5fcb61f8187ff', date: '1980-04-02T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: "EL SALVADOR.  Gunmen in San Salvador failed in an attempt to assassinate Carlos Lemus Gallardo, Guatemala's ambassador to El Salvador.  The gunmen fired on Lemus from a pickup truck but the diplomat and his bodyguard reportedly fought them off with submachine guns.  The attack was the third in three years."});

