MATCH (a:Attack {id: 'a8251fb1b6c34d66b2b17f03f34a2629'}), (l:Location {country: 'Federal Republic Of Germany', city: 'Berlin'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: '67078f17baa64447a61b95393b49bc90', date: '1978-04-05T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'PAKISTAN.  A terrorist riding a motorcycle lobbed a hand grenade over the wall of the U.S. Consulate in Lahore, but it exploded harmlessly in the courtyard.  No damage was done and no arrests were made. Observers believed the attack may have been by individuals seeking to avenge the death sentence of former Premier Zulfiqar Ali Bhutto (who had accused the U.S. of causing his overthrow to prevent the building of a Pakistani nuclear reprocessing plant).'});

MATCH (a:Attack {id: '67078f17baa64447a61b95393b49bc90'}), (l:Location {country: 'Pakistan', city: 'Lahore'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: 'bb356ff1a93f42c38a8720bb69c4101b', date: '1978-04-19T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'COLOMBIA.  An explosive device was found under a car used by a Peace Corps employee in Bogota.  A guard noticed the package and pulled it from beneath the vehicle just before it exploded.  M-19 had been written in the dust on a side window of the car.  There were no injuries; damage to the vehicle was minimal.'});

MATCH (a:Attack {id: 'bb356ff1a93f42c38a8720bb69c4101b'}), (l:Location {country: 'Colombia', city: 'Bogota'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: '61113938482e4d978bc1c988e8e2fa50', date: '1978-04-26T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'ISRAEL.  Two West German citizens were killed and several others injured when a grenade which was thrown through an open bus window exploded.  They were returning to Jerusalem from an outing in Galilee when the attack took place.  Several Arabs were taken into custody during the search for the perpetrators.'});

MATCH (a:Attack {id: '61113938482e4d978bc1c988e8e2fa50'}), (l:Location {country: 'Israel', city: 'Jerusalem'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: '47775457b3de4416b7bb7f236080d492', date: '1978-05-04T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'FRANCE.  M. Henri Curiel, an Egyptian political refugee, was assassinated in Paris.  An anonymous caller phoned the agency France-Presse stating Today...the agent of KGB, Henry Curiel, militant of the Arab cause, traitor to France who adopted him, has been executed....  The caller identified himself as a member of DELTA, a right-wing group responsible for many assassinations in France. Curiel was a known supporter of various liberation movements in the Third World and reputedly had assisted terrorist groups.  He had been expelled from France but had obtained a delay in the expulsion.'});

MATCH (a:Attack {id: '47775457b3de4416b7bb7f236080d492'}), (l:Location {country: 'France', city: 'Paris'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: '3cded224e9f341f7bfd578555ab60de2', date: '1978-05-05T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'TURKEY.  An explosive was detonated between two buildings in Ankara. One of the buildings houses the U.S. International Communication Agency.  No injuries resulted and property damage was limited to broken windows.  No person or group claimed responsibility.'});

