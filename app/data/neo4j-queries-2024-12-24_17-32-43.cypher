MATCH (a:Attack {id: '3e157dc993f049e990922b6c73a5d443'}), (l:Location {country: 'Yemen', city: 'Sanaa'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: '0ff2a49342394f7798b06802a2d10022', date: '1986-04-26T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'MEXICO.  A caller tipped off police about a bomb in a car parked in front of the U.S. Embassy in Mexico City.  The sophisticated device was defused 15 minutes before it was set to go off.  The Simon Bolivar Anti-Imperialist Commando claimed responsibility for the action, linking it to the U.S. air strike in Libya.'});

MATCH (a:Attack {id: '0ff2a49342394f7798b06802a2d10022'}), (l:Location {country: 'Mexico', city: 'Mexico City'}) CREATE (a)-[:OCCURRED_AT]->(l);

