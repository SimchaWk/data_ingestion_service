CREATE (a:Attack {id: '23f71fa59e7c4f2e8d337bc092e546f4', date: '1986-04-15T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'JAPAN.  A crude rocket-launching device being transported by truck near the American-operated Yokota Air Base outside of Tokyo exploded, causing no injuries or damage.  A fin of one rocket was discovered on the base.  Five empty rocket-launching tubes were found inside the burned out truck.'});

