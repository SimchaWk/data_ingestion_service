CREATE (a:Attack {id: '695660415714442ba81c12a50a6b9cb0', date: '1978-02-07T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: "PHILIPPINES.  Three Ananda Marga members stabbed an Indian embassy official who was in the embassy at the time.  The embassy official, who was the personal secretary to the charge d'affaires, was stabbed in the chest and throat but survived the attack."});

