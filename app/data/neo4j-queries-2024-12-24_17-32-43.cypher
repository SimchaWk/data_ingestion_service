MATCH (a:Attack {id: '9ee23c8d7103407395cf4184562b6a91'}), (l:Location {country: 'United Kingdom', city: 'London'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: 'c21e2e9482094bbcaaecbd1bc3748a24', date: '1983-07-20T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'LEBANON.  A large explosion targeted a beach hotel resort complex near West Beirut which is known to be frequented by foreigners and wealthy Lebanese.  At least three people were killed and two Italian soldiers were injured in the blast, which supposedly originated from within the resort complex.  There was extensive damage to the installation which is located in an area presently under Italian control.  Although no group claimed responsibility, it is believed to have been carried out by the group Front to Liberate Lebanon from Foreigners.'});

MATCH (a:Attack {id: 'c21e2e9482094bbcaaecbd1bc3748a24'}), (l:Location {country: 'Lebanon', city: 'Beirut'}) CREATE (a)-[:OCCURRED_AT]->(l);

CREATE (a:Attack {id: 'd30a016db3794d668420a74e170e5373', date: '1983-07-20T00:00:00', data_source: 'only_in_secondary', num_perpetrators_captured: 0, description: 'FRANCE.  ASALA threatened more attacks against French territory and government organizations.  The Perrache railway station in Lyons was evacuated following a bomb threat by a telephone caller who said he was part of the terrorist group.It is unclear whether or not a bomb was found but most probably not.'});

MATCH (a:Attack {id: 'd30a016db3794d668420a74e170e5373'}), (l:Location {country: 'France', city: 'Lyon'}) CREATE (a)-[:OCCURRED_AT]->(l);

